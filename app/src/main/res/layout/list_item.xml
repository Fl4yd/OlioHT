<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <ImageView
        android:id="@+id/imagebackground"
        android:layout_width="wrap_content"
        app:flow_horizontalBias="0.5"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/profile_taustanelio_bg"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/list_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="150dp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="465dp"
        app:layout_constraintTop_toTopOf="@id/imagebackground"
        app:layout_constraintLeft_toLeftOf="@id/imagebackground"
        app:layout_constraintRight_toRightOf="@id/imagebackground"
        app:layout_constraintBottom_toBottomOf="@id/imagebackground"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="172dp"
        android:layout_height="39dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:text="Your score: string \nUser score: string"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@id/imagebackground"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintLeft_toRightOf="@id/list_image"
        app:layout_constraintRight_toRightOf="@id/imagebackground"
        app:layout_constraintTop_toTopOf="@id/imagebackground"
        app:layout_constraintVertical_bias="0.497" />

    <TextView
        android:id="@+id/movie_name"
        android:layout_width="170dp"
        android:layout_height="41dp"
        android:text=""
        android:textSize="18sp"
        android:textStyle="bold"
        android:layout_marginTop="60dp"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="78dp"
        app:layout_constraintBottom_toTopOf="@id/textView11"
        app:layout_constraintLeft_toRightOf="@id/list_image"
        app:layout_constraintRight_toRightOf="@id/imagebackground"
        app:layout_constraintTop_toTopOf="@id/imagebackground"
        />

</androidx.constraintlayout.widget.ConstraintLayout>